@model KaryawanApp.Models.Data_User
@{
}
<form asp-action="SignUp" asp-controller="Home" method="post">
    <div class="form-group">
        <label>Email</label>
        <input asp-for="USR_EMAIL" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Password</label>
        <input asp-for="USR_PASS" type="password" class="form-control" required />
    </div>
    <button type="submit" class="btn btn-primary">Sign Up</button>
</form>
